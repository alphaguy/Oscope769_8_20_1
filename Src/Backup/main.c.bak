int main(void)
{

  /* USER CODE BEGIN 1 */
//  HAL_NVIC_DisableIRQ(EXTI4_IRQn);// do not let trigger interrupt mess you up
  
  /* Configure the MPU attributes as Write Through */
  MPU_Config();
	
  /* Invalidate I-Cache : ICIALLU register*/
  SCB_InvalidateICache();	
  
  /* Enable branch prediction */
  SCB->CCR |= (1 <<18); 
  __DSB();
  
  /* Invalidate I-Cache : ICIALLU register*/
  SCB_InvalidateICache();    
  
  /* Enable I-Cache */
  SCB_EnableICache();        
  
  SCB_InvalidateDCache();   
  SCB_EnableDCache();        
  /* USER CODE END 1 */

  /* MCU Configuration----------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_DMA_Init();
  MX_ADC1_Init();
  MX_ADC3_Init();
  MX_DMA2D_Init();
  MX_DSIHOST_DSI_Init();
  MX_FMC_Init();
  MX_HDMI_CEC_Init();
  MX_I2C1_Init();
  MX_I2C4_Init();
  MX_RTC_Init();
  MX_SPDIFRX_Init();
  MX_SPI2_Init();
  MX_LTDC_Init();
  MX_TIM1_Init();
  MX_QUADSPI_Init();

  /* USER CODE BEGIN 2 */
  MX_USB_DEVICE_Init();
  BSP_SDRAM_Init();
  TS_Init(); // Initialize touch screen

  /* USER CODE END 2 */

  /* USER CODE BEGIN RTOS_MUTEX */
  /* add mutexes, ... */
  /* USER CODE END RTOS_MUTEX */

  /* Create the semaphores(s) */
  /* definition and creation of ButtPressed */
  osSemaphoreDef(ButtPressed);
  ButtPressedHandle = osSemaphoreCreate(osSemaphore(ButtPressed), 1);

  /* definition and creation of TimerDone */
  osSemaphoreDef(TimerDone);
  TimerDoneHandle = osSemaphoreCreate(osSemaphore(TimerDone), 1);

  /* definition and creation of Trigger */
  osSemaphoreDef(Trigger);
  TriggerHandle = osSemaphoreCreate(osSemaphore(Trigger), 1);

  /* definition and creation of ADCdone */
  osSemaphoreDef(ADCdone);
  ADCdoneHandle = osSemaphoreCreate(osSemaphore(ADCdone), 1);

  /* definition and creation of DrawDone */
  osSemaphoreDef(DrawDone);
  DrawDoneHandle = osSemaphoreCreate(osSemaphore(DrawDone), 1);

  /* USER CODE BEGIN RTOS_SEMAPHORES */
  /* add semaphores, ... */
  /* USER CODE END RTOS_SEMAPHORES */

  /* Create the timer(s) */
  /* definition and creation of AutoRun */
  osTimerDef(AutoRun, AutoTrig);
  AutoRunHandle = osTimerCreate(osTimer(AutoRun), osTimerPeriodic, NULL);

  /* definition and creation of PressCheck */
  osTimerDef(PressCheck, PressTimeout);
  PressCheckHandle = osTimerCreate(osTimer(PressCheck), osTimerPeriodic, NULL);

  /* USER CODE BEGIN RTOS_TIMERS */
  /* start timers, add new ones, ... */
  /* USER CODE END RTOS_TIMERS */

  /* Create the thread(s) */
  /* definition and creation of printTask */
  osThreadDef(printTask, StartPrintTask, osPriorityNormal, 0, 256);
  printTaskHandle = osThreadCreate(osThread(printTask), NULL);

  /* definition and creation of Touch */
  osThreadDef(Touch, TouchHandlerRTOS, osPriorityHigh, 0, 256);
  TouchHandle = osThreadCreate(osThread(Touch), NULL);

  /* definition and creation of GUItask */
  osThreadDef(GUItask, GUItaskStart, osPriorityRealtime, 0, 1024);
  GUItaskHandle = osThreadCreate(osThread(GUItask), NULL);

  /* definition and creation of TScal */
  osThreadDef(TScal, TScalMode, osPriorityLow, 0, 256);
  TScalHandle = osThreadCreate(osThread(TScal), NULL);

  /* definition and creation of StateMachine */
  osThreadDef(StateMachine, StatesStart, osPriorityHigh, 0, 512);
  StateMachineHandle = osThreadCreate(osThread(StateMachine), NULL);

  /* definition and creation of Normal */
  osThreadDef(Normal, NormalMode, osPriorityNormal, 0, 256);
  NormalHandle = osThreadCreate(osThread(Normal), NULL);

  /* USER CODE BEGIN RTOS_THREADS */
  /* add threads, ... */
  /* USER CODE END RTOS_THREADS */

  /* Create the queue(s) */
  /* definition and creation of GUIQueue */
  osMessageQDef(GUIQueue, 16, uint16_t*);
  GUIQueueHandle = osMessageCreate(osMessageQ(GUIQueue), NULL);

  /* definition and creation of EventQueue */
  osMessageQDef(EventQueue, 16, uint16_t);
  EventQueueHandle = osMessageCreate(osMessageQ(EventQueue), NULL);

  /* definition and creation of printQueue */
  osMessageQDef(printQueue, 16, void*);
  printQueueHandle = osMessageCreate(osMessageQ(printQueue), NULL);

  /* USER CODE BEGIN RTOS_QUEUES */
  /* add queues, ... */
  RTOSinitPlugin();
  /* USER CODE END RTOS_QUEUES */
 

  /* Start scheduler */
  osKernelStart();
  
  /* We should never get here as control is now taken by the scheduler */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
  /* USER CODE END WHILE */

  /* USER CODE BEGIN 3 */

  }
  /* USER CODE END 3 */

